<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/content_map_screen"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="maximedelange.clickgame.Screens.MapScreen"
    tools:showIn="@layout/activity_map_screen">

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/backgroundmap"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:id="@+id/imgMap"
        android:layout_alignParentBottom="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:adjustViewBounds="true"
        android:scaleType="fitXY" />

    <ImageButton
        android:layout_width="25dp"
        android:layout_height="25dp"
        app:srcCompat="@mipmap/ic_launcher"
        android:id="@+id/btnlevel10"
        android:layout_above="@+id/btnlevel9"
        android:layout_toLeftOf="@+id/btnlevel5"
        android:layout_toStartOf="@+id/btnlevel5"
        android:layout_marginBottom="21dp"
        android:scaleType="fitXY"
        android:adjustViewBounds="true" />

    <ImageButton
        app:srcCompat="@mipmap/ic_launcher"
        android:layout_marginLeft="52dp"
        android:layout_marginStart="52dp"
        android:id="@+id/btnlevel1"
        android:layout_width="25dp"
        android:layout_height="25dp"
        android:adjustViewBounds="true"
        android:scaleType="fitStart"
        android:layout_marginBottom="27dp"
        android:layout_above="@+id/btnlevel6"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <ImageButton
        android:layout_width="25dp"
        android:layout_height="25dp"
        app:srcCompat="@mipmap/ic_launcher"
        android:id="@+id/btnlevel2"
        android:layout_marginLeft="42dp"
        android:layout_marginStart="42dp"
        android:scaleType="fitStart"
        android:adjustViewBounds="true"
        android:layout_above="@+id/btnlevel1"
        android:layout_toRightOf="@+id/btnlevel1"
        android:layout_toEndOf="@+id/btnlevel1" />

    <ImageButton
        android:layout_width="25dp"
        android:layout_height="25dp"
        app:srcCompat="@mipmap/ic_launcher"
        android:layout_marginLeft="48dp"
        android:layout_marginStart="48dp"
        android:id="@+id/btnlevel3"
        android:layout_marginTop="10dp"
        android:scaleType="fitStart"
        android:adjustViewBounds="true"
        android:layout_alignTop="@+id/btnlevel2"
        android:layout_toRightOf="@+id/btnlevel2"
        android:layout_toEndOf="@+id/btnlevel2" />

    <ImageButton
        android:layout_width="25dp"
        android:layout_height="25dp"
        app:srcCompat="@mipmap/ic_launcher"
        android:layout_marginLeft="39dp"
        android:layout_marginStart="39dp"
        android:id="@+id/btnlevel4"
        android:scaleType="fitStart"
        android:adjustViewBounds="true"
        android:layout_alignTop="@+id/btnlevel3"
        android:layout_toRightOf="@+id/btnlevel3"
        android:layout_toEndOf="@+id/btnlevel3"
        android:layout_marginTop="7dp" />

    <ImageButton
        android:layout_width="25dp"
        android:layout_height="25dp"
        app:srcCompat="@mipmap/ic_launcher"
        android:id="@+id/btnlevel7"
        android:scaleType="fitStart"
        android:adjustViewBounds="true"
        android:layout_alignTop="@+id/btnlevel6"
        android:layout_alignLeft="@+id/btnlevel2"
        android:layout_alignStart="@+id/btnlevel2"
        android:layout_marginTop="9dp" />

    <ImageButton
        android:layout_width="25dp"
        android:layout_height="25dp"
        app:srcCompat="@mipmap/ic_launcher"
        android:layout_marginTop="82dp"
        android:id="@+id/btnlevel8"
        android:scaleType="fitStart"
        android:adjustViewBounds="true"
        android:cropToPadding="true"
        android:layout_marginRight="7dp"
        android:layout_marginEnd="7dp"
        android:layout_below="@+id/btnlevel6"
        android:layout_alignRight="@+id/btnlevel7"
        android:layout_alignEnd="@+id/btnlevel7" />

    <ImageButton
        android:layout_width="25dp"
        android:layout_height="25dp"
        app:srcCompat="@mipmap/ic_launcher"
        android:id="@+id/btnlevel9"
        android:scaleType="fitStart"
        android:adjustViewBounds="true"
        android:layout_alignTop="@+id/btnlevel8"
        android:layout_alignRight="@+id/btnlevel4"
        android:layout_alignEnd="@+id/btnlevel4"
        android:layout_marginRight="17dp"
        android:layout_marginEnd="17dp" />

    <ImageButton
        android:layout_width="25dp"
        android:layout_height="25dp"
        app:srcCompat="@mipmap/ic_launcher"
        android:id="@+id/btnlevel6"
        android:scaleType="fitStart"
        android:adjustViewBounds="true"
        android:layout_centerVertical="true"
        android:layout_alignRight="@+id/btnlevel3"
        android:layout_alignEnd="@+id/btnlevel3"
        android:layout_marginRight="10dp"
        android:layout_marginEnd="10dp" />

    <ImageButton
        android:layout_width="25dp"
        android:layout_height="25dp"
        app:srcCompat="@mipmap/ic_launcher"
        android:id="@+id/btnlevel5"
        android:scaleType="fitStart"
        android:adjustViewBounds="true"
        android:layout_above="@+id/btnlevel7"
        android:layout_alignLeft="@+id/btnlevel4"
        android:layout_alignStart="@+id/btnlevel4" />

</RelativeLayout>
